{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandleMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "feed_price"
      ],
      "properties": {
        "feed_price": {
          "type": "object",
          "required": [
            "price"
          ],
          "properties": {
            "price": {
              "description": "The price of asset with base token",
              "allOf": [
                {
                  "$ref": "#/definitions/Decimal"
                }
              ]
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "update_config"
      ],
      "properties": {
        "update_config": {
          "type": "object",
          "properties": {
            "asset_token": {
              "anyOf": [
                {
                  "$ref": "#/definitions/HumanAddr"
                },
                {
                  "type": "null"
                }
              ]
            },
            "base_denom": {
              "type": [
                "string",
                "null"
              ]
            },
            "owner": {
              "anyOf": [
                {
                  "$ref": "#/definitions/HumanAddr"
                },
                {
                  "type": "null"
                }
              ]
            },
            "price_multiplier": {
              "anyOf": [
                {
                  "$ref": "#/definitions/Decimal"
                },
                {
                  "type": "null"
                }
              ]
            },
            "quote_denom": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
