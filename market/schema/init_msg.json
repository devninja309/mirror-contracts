{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "active_commission",
    "asset_oracle",
    "asset_symbol",
    "asset_token",
    "collateral_denom",
    "commission_collector",
    "inactive_commission",
    "liquidity_token",
    "max_minus_spread",
    "max_spread"
  ],
  "properties": {
    "active_commission": {
      "description": "Commission rate for active liquidity provider",
      "allOf": [
        {
          "$ref": "#/definitions/Decimal"
        }
      ]
    },
    "asset_oracle": {
      "description": "Asset oracle address",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    },
    "asset_symbol": {
      "description": "Asset symbol",
      "type": "string"
    },
    "asset_token": {
      "description": "Asset token address",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    },
    "collateral_denom": {
      "description": "Collateral denom",
      "type": "string"
    },
    "commission_collector": {
      "description": "Inactive commission collector",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    },
    "inactive_commission": {
      "description": "Commission rate for mirror token stakers",
      "allOf": [
        {
          "$ref": "#/definitions/Decimal"
        }
      ]
    },
    "liquidity_token": {
      "description": "Liquidity token, required to withdraw liquidity position",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    },
    "max_minus_spread": {
      "description": "Maximum spread to protect trader",
      "allOf": [
        {
          "$ref": "#/definitions/Decimal"
        }
      ]
    },
    "max_spread": {
      "description": "Maximum minus spread to protect arbitrage attack",
      "allOf": [
        {
          "$ref": "#/definitions/Decimal"
        }
      ]
    }
  },
  "definitions": {
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
